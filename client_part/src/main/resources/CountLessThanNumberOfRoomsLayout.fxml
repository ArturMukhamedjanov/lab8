<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.Group?>
<Group fx:id="root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.0.1"  fx:controller="layoutsControllers.CountLessThanNumberOfRoomsLayoutController">
<VBox fx:id="parentBox" prefHeight="75.0" prefWidth="238.0" xmlns="http://javafx.com/javafx/17.0.0.1"  xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="85.0" prefWidth="232.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="textEnterNumberOfRooms" layoutY="14.0" text="Введите количество комнат" />
            <TextField fx:id="enterNumberOfRooms" layoutX="157.0" layoutY="9.0" prefHeight="26.0" prefWidth="77.0" />
            <Button fx:id="submitButton" layoutX="146.0" layoutY="38.0" mnemonicParsing="false" text="Подтвердить" onAction="#submitButtonClicked"/>
            <Button fx:id="cancelButton" layoutX="81.0" layoutY="38.0" mnemonicParsing="false" text="Отмена" onAction="#quit"/>
         </children>
    </AnchorPane>
  </children>
</VBox>
</Group>
