<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ComboBox?>
<Group fx:id="root" xmlns="http://javafx.com/javafx/17.0.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="layoutsControllers.RemoveLowerElementsLayoutController">
<VBox fx:id="parentBox" prefHeight="382.0" prefWidth="269.0" xmlns="http://javafx.com/javafx/17.0.0.1"  xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane fx:id="TextEnterName" maxHeight="-1.0" maxWidth="-1.0" prefHeight="427.0" prefWidth="270.0" VBox.vgrow="ALWAYS">
