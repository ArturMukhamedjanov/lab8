            <TextField fx:id="enterCoordinateY" layoutX="135.0" layoutY="85.0" prefHeight="26.0" prefWidth="108.0" />
            <TextField fx:id="enterArea" layoutX="107.0" layoutY="116.0" prefHeight="26.0" prefWidth="136.0" />
