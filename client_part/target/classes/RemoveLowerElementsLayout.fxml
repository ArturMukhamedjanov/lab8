            <Label fx:id="textEnterNameOfHouse" layoutX="8.0" layoutY="271.0" text="Введите имя дома" />
            <TextField fx:id="enterNameOfHouse" layoutX="114.0" layoutY="266.0" prefHeight="26.0" prefWidth="128.0" />
            <Label fx:id="textEnterYearOfHouse" layoutX="9.0" layoutY="300.0" text="Введите год дома" />
